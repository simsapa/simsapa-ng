<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow JIT compilation for QtWebEngine -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
    
    <!-- Allow unsigned executable memory for QtWebEngine -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Disable library validation for third-party frameworks -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Network access (for web content and API calls) -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <key>com.apple.security.network.server</key>
    <true/>
    
    <!-- File access permissions -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
    
    <!-- Allow printing (if needed) -->
    <key>com.apple.security.print</key>
    <true/>
    
    <!-- Device info access (if needed) -->
    <key>com.apple.security.device.audio-input</key>
    <false/>
    
    <key>com.apple.security.device.camera</key>
    <false/>
</dict>
</plist>
